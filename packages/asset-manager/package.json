{
  "name": "@sugarch/bc-asset-manager",
  "version": "0.2.4",
  "description": "An asset manager for Bondage Club mods, easily add custom assets to the game.",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "type-check": "tsc --noEmit"
  },
  "author": "Da'Inihlus<dynilath@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SugarChain-Studio/sugarch-utilities.git"
  },
  "packageManager": "pnpm@10.6.2",
  "dependencies": {
    "@sugarch/bc-mod-types": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "bc-stubs": "113.0.0",
    "bondage-club-mod-sdk": "^1.2.0",
    "rollup": "^2.0.0",
    "tslib": "^2.8.1",
    "@sugarch/bc-shared-utility": "workspace:*"
  },
  "peerDependencies": {
    "@sugarch/bc-mod-hook-manager": "workspace:*",
    "bondage-club-mod-sdk": "^1.2.0"
  }
}
